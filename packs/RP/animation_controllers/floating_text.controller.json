{
	"format_version": "1.10.0",
	"animation_controllers": {
		"controller.animation.floating_text.visibility": {
			"initial_state": "hidden",
			"states": {
				"hidden": {
					"transitions": [
						{
							"visible": "q.is_on_screen"
						}
					]
				},
				"visible": {
					"transitions": [
						{
							"hidden": "!q.is_on_screen"
						}
					]
				}
			}
		}
	}
}